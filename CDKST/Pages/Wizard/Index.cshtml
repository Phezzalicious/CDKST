@page
@model CDKST.Pages.Wizard.IndexModel
@{
    ViewData["Title"] = "Home page";
}
  <!-- Header -->
  <header class="masthead">
    <div class="container d-flex h-100 align-items-center">
      <div class="mx-auto text-center">
        <h1 class="mx-auto my-0 text-uppercase">Wizard</h1>
        <h2 class="text-white-50 mx-auto mt-2 mb-5">Build your Competency here!</h2>
        <a class="btn btn-primary js-scroll-trigger" asp-area="" asp-page="/Wizard/Page1">Begin</a>
      </div>
    </div>
  </header>

  <!-- About Section -->
  <section id="about" class="about-section text-center">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 mx-auto">
          <h2 class="text-white mb-4">Look at all the elements of a competency</h2>
        </div>
        <div class="col-lg-4 mx-auto">
          <div class="row">
            <a class="btn btn-primary js-scroll-trigger" asp-area="" asp-page="/dispositions/index">Dispositions</a>
          </div>
          <br/>
          <div class="row">
            <a class="btn btn-primary js-scroll-trigger" asp-area="" asp-page="/SkillLevels/index">Skill Levels</a>
          </div>
          <br/>
          <div class="row">
            <a class="btn btn-primary js-scroll-trigger" asp-area="" asp-page="/KnowledgeElements/index">Knowledge Elements</a>
          </div>
          <br/>
        </div>
      </div>
</section>

  
 



